<configuration>
  <system.diagnostics>
    <trace>
      <listeners>
        <add type="Microsoft.WindowsAzure.Diagnostics.DiagnosticMonitorTraceListener, Microsoft.WindowsAzure.Diagnostics, Version=2.4.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35" name="AzureDiagnostics">
          <filter type="" />
        </add>
      </listeners>
    </trace>
  </system.diagnostics>
  <appSettings>
    <add key="webpages:Version" value="3.0.0.0" />
    <add key="webpages:Enabled" value="false" />
    <add key="ClientValidationEnabled" value="true" />
    <add key="UnobtrusiveJavaScriptEnabled" value="true" />
  </appSettings>
  <!--
    For a description of web.config changes see http://go.microsoft.com/fwlink/?LinkId=235367.

    The following attributes can be set on the <httpRuntime> tag.
      <system.Web>
        <httpRuntime targetFramework="4.5.1" />
      </system.Web>
  -->
  <system.web>
    <customErrors mode="Off" />
    <compilation debug="true" targetFramework="4.5.1" />
    <httpRuntime maxRequestLength="048576" useFullyQualifiedRedirectUrl="true" executionTimeout="45" />
    <httpModules>
      <!--<add name="NHibernateSessionModule" type="Lacjam.Core.Infrastructure.Ioc.NHibernateSessionModule,Lacjam.Core"/>-->
    </httpModules>    
  </system.web>
  <system.webServer>
    <validation validateIntegratedModeConfiguration="false" />
    <modules runAllManagedModulesForAllRequests="false">
      <remove name="WebDAVModule" />
      <!--<add name="PerRequestLifestyle" type="Castle.MicroKernel.Lifestyle.PerWebRequestLifestyleModule, Castle.Windsor" />-->
      <!--<add name="PerHttpApplicationLifestyleModule" type="Lacjam.WebApi.Infrastructure.Ioc.PerHttpApplicationLifestyleModule, Lacjam.WebApi" />--><!--<add name="PerHttpApplicationLifestyleModule" type="Lacjam.WebApi.Infrastructure.Ioc.PerHttpApplicationLifestyleModule, Lacjam.WebApi" />-->
      <!--<add name="WindsorModule" type="Lacjam.WebApi.Infrastructure.Ioc.WindsorHttpModule, Lacjam.WebApi" />-->
      <!--<add name="NHibernateSessionModule" type="Lacjam.Core.Infrastructure.Ioc.NHibernateSessionModule,Lacjam.Core"/>-->
    </modules>
    <handlers>
      <remove name="WebDAVModule" />
      <remove name="ExtensionlessUrlHandler-Integrated-4.0" />
      <remove name="OPTIONSVerbHandler" />
      <remove name="TRACEVerbHandler" />
      <add name="ExtensionlessUrlHandler-Integrated-4.0" path="*." verb="*" type="System.Web.Handlers.TransferRequestHandler" preCondition="integratedMode,runtimeVersionv4.0" />
    </handlers>
    <security>
      <requestFiltering>
        <requestLimits maxAllowedContentLength="1073741824" />
      </requestFiltering>
    </security>
        <directoryBrowse enabled="true" />
  </system.webServer>
  <runtime>
